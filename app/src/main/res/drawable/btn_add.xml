<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:top="10dp">
        <shape
            android:shape="oval">
            <gradient
                android:startColor="@color/darkerGray"
                android:endColor="@color/beigeLight"
                android:angle="1"
                android:centerColor="@color/darkerGray"/>
        </shape>
    </item>
    <item android:bottom="10dp">
        <shape
            android:shape="oval">
            <solid android:color="@color/purpleMain"/>
        </shape>
    </item>
    <item android:bottom="220dp" android:top="220dp" android:right="120dp" android:left="120dp" >
        <shape android:shape="rectangle">
            <solid android:color="@color/beigeLight">
            </solid>
        </shape>
    </item>
    <item android:right="220dp" android:left="220dp" android:bottom="120dp" android:top="120dp" >
        <shape android:shape="rectangle">
            <solid android:color="@color/beigeLight">
            </solid>
        </shape>
    </item>
</layer-list>